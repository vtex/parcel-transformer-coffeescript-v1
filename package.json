    "semver": "^7.5.2"
  },
  "devDependencies": {
    "@types/node": "^17.0.8",
    "@vtex/prettier-config": "^1.0.0",
    "eslint": "^7",
    "eslint-config-vtex": "^14.1.2",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
